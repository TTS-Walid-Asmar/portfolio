<div class="row">
  <h2> My Top 5 </h2>
    <% @songs.each do |song| %>
      <div class="well well-lg">
        <h2><%= link_to song.name, song %></h2>
    <h3>by <%= link_to song.artist.name, song.artist %> </h3>
    <p><% "#{song.rating}".to_i.times do |rank| %>
      <span class="glyphicon glyphicon-fire"></span>
        <% end %>
    </p>
    <h3> <span class="glyphicon glyphicon-play-circle"></span></h3>
     </div>
  <% end %>
</div>
